<?php
$students=array("Fathima","Afeef","Don","Arjun","Stefi","Sreelaya","Adarsh","Arun","Marvin","Kannan","Chandrakanth","Dwany","Binoy","Joyal","Jeslin");
asort($students);
echo "<h1>The array sorted using asort function</h1><br>";
print_r($students);
echo"<br>";
arsort($students);
echo"<h1>The array sorted using arsort function</h1><br>";
print_r($students);
?>
